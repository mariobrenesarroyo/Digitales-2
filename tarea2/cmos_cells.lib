library(demo) {
  cell(BUF) {
    area: 3;
    pin(A) { direction: input; }
    pin(Y) { direction: output; function: "A"; }
    delay: 0.1;
  }

  cell(NOT) {
    area: 3;
    pin(A) { direction: input; }
    pin(Y) { direction: output; function: "A'"; }
    delay: 0.1;
  }

  cell(NAND) {
    area: 4;
    pin(A) { direction: input; }
    pin(B) { direction: input; }
    pin(Y) { direction: output; function: "(A*B)'"; }
    delay: 0.2;
  }

  cell(NOR) {
    area: 4;
    pin(A) { direction: input; }
    pin(B) { direction: input; }
    pin(Y) { direction: output; function: "(A+B)'"; }
    delay: 0.2;
  }

  cell(DFF) {
    area: 18;
    pin(C) { direction: input; clock: true; }
    pin(D) { direction: input; }
    pin(Q) { direction: output; function: "C ? D : Q"; }
    delay: 0.3;
  }
}
